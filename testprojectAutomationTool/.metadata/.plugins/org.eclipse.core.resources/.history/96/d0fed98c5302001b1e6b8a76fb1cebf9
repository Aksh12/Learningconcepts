package basicJavaCoding;

import java.util.Scanner;

public class PrimeNumber {

	public static void main(String[] args) {
		// Prime Number : Number which is divisble by only 1 and itself
		
		Scanner sc = new Scanner(System.in);
		System.out.println("enter number to check if it is PrimeNumber");
		int num = sc.nextInt();
		
		for(int i=0; i<=num; i++) {
		if(num%1==0 && num%num==0 && num%2!=0) {
			System.out.println("Given number is a prime number : " + num);
		}else {
			System.out.println("Given number is not a prime number");
		}
		}

	}

}
