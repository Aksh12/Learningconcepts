package dataprovider;

import org.testng.annotations.Test;

public class DataProviderExample {
	
	@Test
	public void calculate(int a, int b) {
		int sum=(a+b);
		System.out.println("a+b =  " + sum);
	}
	
	public Object[][] inputData() {
		
		return new Object[][] {
			{22,23},
			{1,2}
		};
		
	}

}
