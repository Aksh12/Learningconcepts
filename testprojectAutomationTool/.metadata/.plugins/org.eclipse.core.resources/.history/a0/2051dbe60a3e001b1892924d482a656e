package assertions;

import org.testng.Assert;
import org.testng.annotations.Test;

public class Asser_Test1 {
	
	@Test
	public void testNumber() {
		Assert.assertTrue(numberGreater(5, 6), "Number x is grater");
	}
	
	public boolean numberGreater(int x, int y) {
	
		boolean result;
		
		if(x>y) {
			result = true;
		}else {
			result = false;
		}
		
		return result;
	}

}
